{
  "name": "@gothd/kit-componentes",
  "version": "0.6.1",
  "description": "Kit de componentes UI disponÃ­vel como Web Components e pacote React.",
  "types": "dist/react/components/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/react/components/index.js",
      "types": "./dist/react/components/index.d.ts"
    },
    "./ui": {
      "import": "./dist/react/components/ui/index.js",
      "types": "./dist/react/components/ui/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/react/hooks/index.js",
      "types": "./dist/react/hooks/index.d.ts"
    },
    "./utils": {
      "import": "./dist/react/utils/index.js",
      "types": "./dist/react/utils/index.d.ts"
    },
    "./styles": {
      "import": "./dist/react/styles/index.js",
      "types": "./dist/react/styles/index.d.ts"
    },
    "./styles.css": "./dist/react/styles/index.css",
    "./tailwind.css": "./dist/react/styles/tailwind.css"
  },
  "files": [
    "dist/react"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run build:css && cross-env NODE_ENV=development vite build --mode development --config vite.web.config.ts && vite --config vite.playground.config.ts",
    "preview": "vite preview --config vite.playground.config.ts",
    "build:playground": "tsc -p tsconfig.playground.json && vite build --config vite.playground.config.ts",
    "preview:playground": "vite preview --config vite.playground.config.ts",
    "build:copy-css": "cpx \"src/styles/*.css\" dist/react/styles",
    "build:react": "cross-env NODE_ENV=production tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && npm run build:copy-css",
    "build:css": "node generate-css.mjs",
    "build:wc:core": "vite build --config vite.web.config.ts",
    "build:wc:components": "node build-wc-components.mjs",
    "build:wc": "npm run build:css && npm run build:wc:core && npm run build:wc:components",
    "preview:wc": "vite preview --config vite.web.config.ts",
    "build:prod": "npm run clean && npm run build:react && npm run build:wc",
    "release": "npm run build:prod && npm publish --access public"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@emailjs/browser": "^4.4.1",
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "cpx2": "^8.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "react-to-webcomponent": "^2.0.1",
    "rimraf": "^6.1.0",
    "tailwindcss": "3.4.18",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gothd/kit-componentes.git"
  },
  "author": "Ruan ruanalt@aol.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gothd/kit-componentes/issues"
  },
  "homepage": "https://github.com/gothd/kit-componentes#readme"
}
