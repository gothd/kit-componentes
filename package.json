{
  "name": "kit-componentes",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/kit-componentes.umd.js",
  "module": "dist/kit-componentes.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib/kit-componentes.es.js",
      "require": "./dist/lib/kit-componentes.umd.js"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js"
    },
    "./styles.css": "./dist/styles/index.css",
    "./styles": {
      "import": "./dist/styles/index.js",
      "require": "./dist/styles/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "hooks": [
        "dist/hooks/index.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "styles": [
        "dist/styles/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.playground.config.ts",
    "build:playground": "tsc -p tsconfig.playground.json && vite build --config vite.playground.config.ts",
    "preview:playground": "vite preview --config vite.playground.config.ts",
    "build:bundles": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:copy-css": "cpx \"src/styles/index.css\" dist/styles",
    "build:lib": "npm run build:bundles && npm run build:types && vite build && npm run build:copy-css",
    "preview:lib": "vite preview",
    "build:css": "npx postcss ./src/styles/index.css -o ./src/styles/tailwind.flat.css",
    "build:wc": "npm run build:css && vite build --config vite.web.config.ts",
    "preview:wc": "vite preview --config vite.web.config.ts",
    "build:prod": "npm run build:lib && npm run build:wc"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@benface/tailwindcss-reset": "^3.1.1",
    "@emailjs/browser": "^4.4.1",
    "@eslint/js": "^9.36.0",
    "@tailwindcss/cli": "^4.1.16",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "cpx2": "^8.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-preset-env": "^10.4.0",
    "react-to-webcomponent": "^2.0.1",
    "tailwindcss": "^4.1.16",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
